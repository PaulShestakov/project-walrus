{
  "name": "wiki-server",
  "script": "./dist/server/index.js",
  "node_args": ["--inspect"],
  "watch": true,
  "ignore_watch": ["node_modules", "./dist/uploads"],
  "detached": true,

  "autorestart": true,
  "max_restarts": 100,
  "restart_delay": 1000,

  "out_file": "./dist/devLog/log.txt",

  "env": {
    "PORT": 3000,
    "NODE_ENV": "development"
  },

  "source_map_support": true
}